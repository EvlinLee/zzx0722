<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#e0e0e0">




    <FrameLayout android:id="@+id/main_body1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/layout_toolbar"
        android:background="@color/comm_background"></FrameLayout>


    <LinearLayout
        android:id="@+id/layout_toolbar"
        android:layout_width="match_parent"
        android:layout_height="49dip"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:background="@color/cell_background">

        <RelativeLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/myorder">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:id="@+id/myorder_title"
                android:gravity="center"
                android:text="我的订单"
                android:textColor="#ff4c4c"
                android:textSize="16dip"
                android:enabled="true"/>
            <View
                android:layout_width="1dip"
                android:layout_height="match_parent"
                android:background="@color/line_color"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/teamorder">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:text="团队贡献订单"
                android:gravity="center"
                android:id="@+id/teamorder_title"
                android:textColor="#333333"
                android:textSize="16dip"
                android:enabled="true"/>
        </RelativeLayout>



    </LinearLayout>



</RelativeLayout>