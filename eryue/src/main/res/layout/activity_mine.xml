<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/comm_background"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="270dp">

                <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="190dip"
                android:id="@+id/header_view"
                android:background="@drawable/img_profile_bg2"
                >
                <!--导航栏-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:layout_marginTop="20dp"
                    android:id="@+id/navigation">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/navigation_setting"
                        android:layout_centerVertical="true"
                        android:background="@drawable/icon_set"
                        android:layout_marginLeft="10dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="个人中心"
                        android:layout_centerInParent="true"
                        android:textColor="#ffffff"
                        android:textSize="17sp"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/navigation_message"
                        android:layout_centerVertical="true"
                        android:background="@drawable/icon_message"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="15dip"/>

                    

                </RelativeLayout>
                <!--未登录状态下的头部视图-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="150dip"
                    android:id="@+id/headview_no_login"
                    android:layout_below="@+id/navigation"
                    >
                    <ImageView
                        android:layout_width="50dip"
                        android:layout_height="50dip"
                        android:id="@+id/header_imageview_no_login"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="17dip"
                        android:background="@drawable/ic_launcher"/>
                </RelativeLayout>

                <!--登录状态下的头部视图-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="150dip"
                    android:id="@+id/headview_login"
                    android:layout_below="@+id/navigation"
                    >
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="100dip"
                        android:id="@+id/top_view">
                        <ImageView
                            android:layout_width="50dip"
                            android:layout_height="50dip"
                            android:id="@+id/header_imageview_login"
                            android:layout_marginLeft="15dip"
                            android:src="@drawable/ic_launcher"
                            android:layout_marginTop="15dip"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/name"
                            android:text="猪猪女孩"
                            android:textSize="17sp"
                            android:layout_marginTop="16dip"
                            android:layout_marginLeft="10dip"
                            android:layout_toRightOf="@+id/header_imageview_login"
                            android:textColor="#ffffff"/>

                        <!--已绑定微信-->
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/bindwx"
                            android:textSize="11sp"
                            android:drawableLeft="@drawable/profile_icon_14"
                            android:text="已绑定微信"
                            android:drawablePadding="3dp"
                            android:layout_below="@+id/name"
                            android:layout_toRightOf="@+id/header_imageview_login"
                            android:layout_marginLeft="6dp"
                            android:layout_marginTop="6dp"
                            android:padding="3dp"
                            android:background="@drawable/bg_bindwx_shape"
                            android:textColor="#ffffff"/>

                        <!--签到-->
                        <RelativeLayout
                            android:layout_width="60dip"
                            android:layout_height="24dip"
                            android:id="@+id/qiandao"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="35dip"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="15dip"
                            android:alpha="0.9"
                            android:background="@drawable/hehuoren_bg">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="签到"
                                android:layout_centerInParent="true"
                                android:layout_alignParentLeft="true"
                                android:layout_marginLeft="12dip"
                                android:textColor="#ff1a40"
                                android:textSize="13dip"/>
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="8dip"
                                android:layout_centerVertical="true"
                                android:background="@drawable/profile_icon_arrow_red"/>


                        </RelativeLayout>

                    </RelativeLayout>


                </RelativeLayout>

            </RelativeLayout>
                <!--未登录状态下显示的选项-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dip"
                    android:id="@+id/cell_contain_no_login"
                    android:background="@drawable/lv_item_shape"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:visibility="gone"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="20dp"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:id="@+id/liulanzuji">
                        <ImageView
                            android:layout_width="22dip"
                            android:layout_height="22dip"
                            android:id="@+id/image0"
                            android:background="@drawable/user_icon_1"
                            android:layout_centerHorizontal="true"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="浏览足迹"
                            android:layout_marginTop="10dip"
                            android:textSize="13dip"
                            android:textColor="#666666"
                            android:layout_below="@+id/image0"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:id="@+id/tuiguangyaoqing">
                        <ImageView
                            android:layout_width="22dip"
                            android:layout_height="22dip"
                            android:id="@+id/image1"
                            android:background="@drawable/user_icon_2"
                            android:layout_centerHorizontal="true"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="推广邀请"
                            android:layout_marginTop="10dip"
                            android:textSize="13dip"
                            android:textColor="#666666"
                            android:layout_below="@+id/image1"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:id="@+id/shiyongbangzhu">
                        <ImageView
                            android:layout_width="22dip"
                            android:layout_height="22dip"
                            android:id="@+id/image2"
                            android:background="@drawable/user_icon_3"
                            android:layout_centerHorizontal="true"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="使用帮助"
                            android:layout_marginTop="10dip"
                            android:textSize="13dip"
                            android:textColor="#666666"
                            android:layout_below="@+id/image2"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:id="@+id/contact_server">
                        <ImageView
                            android:layout_width="22dip"
                            android:layout_height="22dip"
                            android:id="@+id/image3"
                            android:background="@drawable/user_icon_4"
                            android:layout_centerHorizontal="true"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="联系客服"
                            android:layout_marginTop="10dip"
                            android:textSize="13dip"
                            android:textColor="#666666"
                            android:layout_below="@+id/image3"
                            android:layout_centerHorizontal="true"
                            />
                    </RelativeLayout>

                </LinearLayout>

                <!--登录显示-->
