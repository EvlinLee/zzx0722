                                android:layout_marginTop="5dip"
                                android:text="0"
                                android:textColor="#333333"
                                android:textSize="16dip"
                                android:layout_centerHorizontal="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="0dip"
                                android:text="预估积分"
                                android:textSize="12dip"
                                android:textColor="#333333"
                                android:id="@+id/text_view1"
                                android:layout_below="@+id/yugujifen"
                                android:layout_centerHorizontal="true"/>

                            <View
                                android:layout_width="1dp"
                                android:layout_height="20dip"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@id/text_view1"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="10dp"
                                android:layout_alignParentTop="true"
                                android:background="#F5F5F5" />
                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/leijiyouxiao"
                                android:layout_marginTop="5dip"
                                android:text="0"
                                android:textColor="#333333"
                                android:textSize="16dip"
                                android:layout_centerHorizontal="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="0dip"
                                android:text="累计有效"
                                android:textSize="12dip"
                                android:textColor="#333333"
                                android:id="@+id/text_view2"
                                android:layout_below="@+id/leijiyouxiao"
                                android:layout_centerHorizontal="true"/>

                            <View
                                android:layout_width="1dp"
                                android:layout_height="20dip"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@id/text_view2"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="10dp"
                                android:layout_alignParentTop="true"
                                android:background="#F5F5F5" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/leijiduihuan"
                                android:layout_marginTop="5dip"
                                android:text="000"
                                android:textColor="#333333"
                                android:textSize="16dip"
                                android:layout_centerHorizontal="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="0dip"
                                android:text="累计兑换"
                                android:textColor="#333333"
                                android:id="@+id/text_view3"
                                android:textSize="12dip"
                                android:layout_below="@+id/leijiduihuan"
                                android:layout_centerHorizontal="true"/>

                            <View
                                android:layout_width="1dp"
                                android:layout_height="20dip"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@id/text_view3"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="10dp"
                                android:layout_alignParentTop="true"
                                android:background="#F5F5F5" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/zhanghuyue"
                                android:layout_marginTop="5dip"
                                android:text="0"
                                android:textColor="#333333"
                                android:textSize="16dip"
                                android:layout_centerHorizontal="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="0dip"
                                android:text="账户余额"
                                android:textColor="#333333"
                                android:textSize="12dip"
                                android:layout_below="@+id/zhanghuyue"
                                android:layout_centerHorizontal="true"/>

                        </RelativeLayout>


                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingTop="20dp"

                        >
                        <RelativeLayout
                            android:layout_width="0dip"
                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:gravity="center_vertical"
                            android:id="@+id/wodetuandui">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/image00"
                                android:background="@drawable/profile_icon_1"
                                android:layout_centerHorizontal="true"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="我的团队"
                                android:layout_marginTop="5dp"
                                android:textSize="13dip"
                                android:textColor="#666666"
                                android:layout_below="@+id/image00"
                                android:layout_centerHorizontal="true"/>
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dip"
                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:gravity="center_vertical"
                            android:id="@+id/dingdanxiangqing">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/image01"
                                android:background="@drawable/profile_icon_2"
                                android:layout_centerHorizontal="true"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="订单详情"
                                android:layout_marginTop="5dp"
                                android:textSize="13dip"
                                android:textColor="#666666"
                                android:layout_below="@+id/image01"
                                android:layout_centerHorizontal="true"/>
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:id="@+id/tuidanxiangqing"
                            android:layout_weight="1">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/image02"
                                android:background="@drawable/profile_icon_3"
                                android:layout_centerHorizontal="true"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="退单详情"
                                android:layout_marginTop="5dp"
                                android:textSize="13dip"
                                android:textColor="#666666"
                                android:layout_below="@+id/image02"
                                android:layout_centerHorizontal="true"/>

                            <View
                                android:layout_width="1dp"
                                android:layout_height="20dip"
                                android:layout_centerVertical="true"
                                android:layout_alignParentRight="true"
                                android:background="#ffffff" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dip"
                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:gravity="center_vertical"
                            android:id="@+id/zhanghutixian">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/image03"
                                android:background="@drawable/profile_icon_4"
                                android:layout_centerHorizontal="true"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="账户提现"
                                android:layout_marginTop="5dp"
                                android:textSize="13dip"
                                android:textColor="#666666"
                                android:layout_below="@+id/image03"
                                android:layout_centerHorizontal="true"/>
                        </RelativeLayout>


                    </LinearLayout>
                </LinearLayout>


            </RelativeLayout>


            <!--登录状态下显示的选项-->
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dip"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/lv_item_shape"
                android:id="@+id/button_list_login"
                android:orientation="vertical"
                >
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="15dp"
                    android:layout_gravity="center_horizontal">
                    <TextView
                        android:drawableLeft="@drawable/profile_icon_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="我的收藏"
                        android:textSize="14sp"
                        android:textColor="#666666"
                        android:drawablePadding="15dp"
                        android:layout_marginBottom="10dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerHorizontal="true"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_setting_arrow"
                        android:layout_marginBottom="11dp"
                        android:layout_alignParentRight="true"
                        />
                    <View
                        android:layout_height="1px"
                        android:layout_width="match_parent"
                        android:background="#F5F5F5"
                        android:layout_alignParentBottom="true"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="15dp"
                    android:id="@+id/dingdantijiao"
                    android:layout_gravity="center_horizontal">
                    <TextView
                        android:drawableLeft="@drawable/profile_icon_6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="订单提交"
                        android:textSize="14sp"
                        android:textColor="#666666"
                        android:drawablePadding="15dp"
                        android:layout_marginBottom="10dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerHorizontal="true"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_setting_arrow"
                        android:layout_marginBottom="11dp"
                        android:layout_alignParentRight="true"
                        />
                    <View
                        android:layout_height="1px"
                        android:layout_width="match_parent"
                        android:background="#F5F5F5"
                        android:layout_alignParentBottom="true"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="15dp"
                    android:id="@+id/liulanzuji_1"
                    android:layout_gravity="center_horizontal">
                    <TextView
                        android:drawableLeft="@drawable/profile_icon_7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="浏览足迹"
                        android:textSize="14sp"
                        android:textColor="#666666"
                        android:drawablePadding="15dp"
                        android:layout_marginBottom="10dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerHorizontal="true"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_setting_arrow"
                        android:layout_marginBottom="11dp"
                        android:layout_alignParentRight="true"
                        />
                    <View
                        android:layout_height="1px"
                        android:layout_width="match_parent"
                        android:background="#F5F5F5"
                        android:layout_alignParentBottom="true"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="15dp"
                    android:id="@+id/tuiguangyaoqing_1"
                    android:layout_gravity="center_horizontal">
                    <TextView
                        android:drawableLeft="@drawable/profile_icon_8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="推广邀请"
                        android:textSize="14sp"
                        android:textColor="#666666"
                        android:drawablePadding="15dp"
                        android:layout_marginBottom="10dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerHorizontal="true"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_setting_arrow"
                        android:layout_marginBottom="11dp"
                        android:layout_alignParentRight="true"
                        />
                    <View
                        android:layout_height="1px"
                        android:layout_width="match_parent"
                        android:background="#F5F5F5"
                        android:layout_alignParentBottom="true"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="15dp"
                    android:id="@+id/shiyongbangzhu_1"
                    android:layout_gravity="center_horizontal">
                    <TextView
                        android:drawableLeft="@drawable/profile_icon_9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="使用帮助"
                        android:textSize="14sp"
                        android:textColor="#666666"
                        android:drawablePadding="15dp"
                        android:layout_marginBottom="10dp"
                        android:layout_alignParentLeft="true"
