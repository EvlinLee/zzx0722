<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:background="@color/comm_background"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="190dip"
                android:id="@+id/header_view"
                >

                <!--导航栏-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:id="@+id/navigation"
                    android:background="#ffffff">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="个人中心"
                        android:layout_centerInParent="true"
                        android:textColor="#000000"
                        android:textSize="18dip"/>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:id="@+id/navigation_item_contain"
                        >
                        <ImageView
                            android:layout_width="25dip"
                            android:layout_height="25dip"
                            android:id="@+id/navigation_help"
                            android:layout_centerVertical="true"
                            android:background="@drawable/icon_help"
                            android:layout_toLeftOf="@+id/navigation_setting"
                            android:layout_marginRight="15dip"/>
                        <ImageView
                            android:layout_width="25dip"
                            android:layout_height="25dip"
                            android:id="@+id/navigation_setting"
                            android:layout_centerVertical="true"
                            android:background="@drawable/icon_setting"
                            android:layout_toLeftOf="@+id/navigation_message"
                            android:layout_marginRight="15dip"
                            />
                        <ImageView
                            android:layout_width="25dip"
                            android:layout_height="25dip"
                            android:id="@+id/navigation_message"
                            android:layout_centerVertical="true"
                            android:background="@drawable/icon_message_grey"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="15dip"/>
                    </RelativeLayout>
                    

                </RelativeLayout>
                <!--未登录状态下的头部视图-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="150dip"
                    android:id="@+id/headview_no_login"
                    android:layout_below="@+id/navigation"
                    android:background="@drawable/bg_mine_header_no_login"
                    android:visibility="gone">
                    <ImageView
                        android:layout_width="50dip"
                        android:layout_height="50dip"
                        android:id="@+id/header_imageview_no_login"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="37dip"
                        android:background="@drawable/ic_launcher"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:minWidth="150dip"
                        android:id="@+id/name_no_login"
                        android:gravity="center"
                        android:layout_below="@+id/header_imageview_no_login"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="15dip"
                        android:textColor="#ffffff"
                        android:text="猪猪虾"/>
                </RelativeLayout>

                <!--登录状态下的头部视图-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="150dip"
                    android:id="@+id/headview_login"
                    android:layout_below="@+id/navigation"
                    android:background="@drawable/bg_mine_header_login">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="100dip"
                        android:id="@+id/top_view">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/type"
                            android:textSize="15dip"
                            android:text="淘其乐快乐赚"
                            android:layout_marginTop="10dip"
                            android:layout_centerHorizontal="true"
                            android:textColor="#ffffff"/>
                        <TextView
                            android:layout_width="55dip"
                            android:layout_height="18dip"
                            android:id="@+id/class_type"
                            android:text="代理"
                            android:gravity="center"
                            android:textSize="12dip"
                            android:textColor="#ff1a40"
                            android:background="@drawable/bg_class_type"
                            android:layout_marginTop="20dip"
                            android:layout_marginLeft="12dip"/>
                        <ImageView
                            android:layout_width="50dip"
                            android:layout_height="50dip"
                            android:id="@+id/header_imageview_login"
                            android:layout_marginLeft="15dip"
                            android:background="@drawable/img_default_contract"
                            android:layout_marginTop="37dip"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/name"
                            android:textSize="17dip"
                            android:text=""
                            android:layout_marginTop="36dip"
                            android:layout_marginLeft="10dip"
                            android:layout_toRightOf="@+id/header_imageview_login"
                            android:textColor="#ffffff"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/id_tv"
                            android:textSize="14dip"
                            android:text="ID"
                            android:layout_marginTop="9dip"
                            android:layout_marginLeft="10dip"
                            android:layout_toRightOf="@+id/header_imageview_login"
                            android:layout_below="@+id/name"
                            android:textColor="#ffffff"/>

                        <!--签到-->
                        <RelativeLayout
                            android:layout_width="60dip"
                            android:layout_height="24dip"
                            android:id="@+id/qiandao"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="35dip"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="15dip"
                            android:alpha="0.9"
                            android:background="@drawable/hehuoren_bg">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="签到"
                                android:layout_centerInParent="true"
                                android:layout_alignParentLeft="true"
                                android:layout_marginLeft="12dip"
                                android:textColor="#ff1a40"
                                android:textSize="13dip"/>
                            <ImageView
                                android:layout_width="15dip"
                                android:layout_height="15dip"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="5dip"
                                android:layout_centerVertical="true"
                                android:background="@drawable/icon_arrow_red"/>


                        </RelativeLayout>

                    </RelativeLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dip"
                        android:id="@+id/down_view"
                        android:orientation="horizontal"
                        android:layout_below="@+id/top_view">

                        <RelativeLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/yugujifen"
                                android:layout_marginTop="5dip"
                                android:text="0"
                                android:textColor="#ffffff"
                                android:textSize="16dip"
                                android:layout_centerHorizontal="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="0dip"
                                android:text="预估积分"
                                android:textColor="#ffffff"
                                android:textSize="12dip"
                                android:layout_below="@+id/yugujifen"
                                android:layout_centerHorizontal="true"/>

                            <View
                                android:layout_width="1dp"
                                android:layout_height="20dip"
                                android:layout_centerVertical="true"
                                android:layout_alignParentRight="true"
                                android:background="#ffffff" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/leijiyouxiao"
                                android:layout_marginTop="5dip"
                                android:text="0"
                                android:textColor="#ffffff"
                                android:textSize="16dip"
                                android:layout_centerHorizontal="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="0dip"
                                android:text="累计有效"
                                android:textColor="#ffffff"
                                android:textSize="12dip"
                                android:layout_below="@+id/leijiyouxiao"
                                android:layout_centerHorizontal="true"/>

                            <View
                                android:layout_width="1dp"
                                android:layout_height="20dip"
                                android:layout_centerVertical="true"
                                android:layout_alignParentRight="true"
                                android:background="#ffffff" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/leijiduihuan"
                                android:layout_marginTop="5dip"
                                android:text="0"
                                android:textColor="#ffffff"
                                android:textSize="16dip"
                                android:layout_centerHorizontal="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="0dip"
                                android:text="累计兑换"
                                android:textColor="#ffffff"
                                android:textSize="12dip"
                                android:layout_below="@+id/leijiduihuan"
                                android:layout_centerHorizontal="true"/>

                            <View
                                android:layout_width="1dp"
                                android:layout_height="20dip"
                                android:layout_centerVertical="true"
                                android:layout_alignParentRight="true"
                                android:background="#ffffff" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/zhanghuyue"
                                android:layout_marginTop="5dip"
                                android:text="0"
                                android:textColor="#ffffff"
                                android:textSize="16dip"
                                android:layout_centerHorizontal="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="0dip"
                                android:text="账户余额"
                                android:textColor="#ffffff"
                                android:textSize="12dip"
                                android:layout_below="@+id/zhanghuyue"
                                android:layout_centerHorizontal="true"/>

                        </RelativeLayout>


                    </LinearLayout>



                </RelativeLayout>


            </RelativeLayout>

           

            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="@color/line_color"
                />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dip"
                android:layout_marginTop="10dip"
                android:background="@color/cell_background"
                android:id="@+id/share_app"
                android:visibility="gone">
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_centerVertical="true"
                    android:background="@drawable/icon_shareapp"
                    android:layout_marginLeft="15dip"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="分享APP"
                    android:textColor="@color/text_color_0"
                    android:textSize="@dimen/txtsize_cell"
                    android:layout_marginLeft="50dip"/>
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dip"
                    android:background="@drawable/icon_disclosure"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="@color/line_color"
                />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/contain_login"
                android:orientation="vertical">
                <!--代理人学习-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dip"
                    android:background="@color/cell_background"
                    android:id="@+id/dailirenxuexi">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="代理人学习"
                        android:textColor="@color/text_color_0"
                        android:textSize="@dimen/txtsize_cell"
                        android:layout_marginLeft="15dip"/>
                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dip"
                        android:background="@drawable/icon_disclosure"/>
                </RelativeLayout>


                <!--我的团队-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dip"
                    android:background="@color/cell_background"
                    android:layout_marginTop="10dip"
                    android:id="@+id/wodetuandui">
                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_centerVertical="true"
                        android:background="@drawable/icon_team1"
                        android:layout_marginLeft="15dip"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="我的团队"
                        android:textColor="@color/text_color_0"
                        android:textSize="@dimen/txtsize_cell"
                        android:layout_marginLeft="50dip"/>
                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dip"
                        android:background="@drawable/icon_disclosure"/>
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="@color/line_color"
                    />
                <!--订单详情-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dip"
                    android:background="@color/cell_background"
                    android:id="@+id/dingdanxiangqing">
                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_centerVertical="true"
                        android:background="@drawable/icon_order_detail"
                        android:layout_marginLeft="15dip"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="订单详情"
                        android:textColor="@color/text_color_0"
                        android:textSize="@dimen/txtsize_cell"
                        android:layout_marginLeft="50dip"/>
                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dip"
                        android:background="@drawable/icon_disclosure"/>
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="@color/line_color"
                    />
                <!--退单详情-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dip"
                    android:background="@color/cell_background"
                    android:id="@+id/tuidanxiangqing">
                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_centerVertical="true"
                        android:background="@drawable/icon_order_withdraw"
                        android:layout_marginLeft="15dip"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="退单详情"
                        android:textColor="@color/text_color_0"
                        android:textSize="@dimen/txtsize_cell"
                        android:layout_marginLeft="50dip"/>
                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dip"
                        android:background="@drawable/icon_disclosure"/>
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="@color/line_color"
                    />
                <!--账户提现-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dip"
                    android:background="@color/cell_background"
                    android:id="@+id/zhanghutixian">
                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_centerVertical="true"
                        android:background="@drawable/icon_cash"
                        android:layout_marginLeft="15dip"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="账户提现"
                        android:textColor="@color/text_color_0"
                        android:textSize="@dimen/txtsize_cell"
                        android:layout_marginLeft="50dip"/>
                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dip"
                        android:background="@drawable/icon_disclosure"/>
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="@color/line_color"
                    />
                <!--订单提交-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dip"
                    android:background="@color/cell_background"
                    android:id="@+id/dingdantijiao">
                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_centerVertical="true"
                        android:background="@drawable/icon_order_submit"
                        android:layout_marginLeft="15dip"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="订单提交"
                        android:textColor="@color/text_color_0"
                        android:textSize="@dimen/txtsize_cell"
                        android:layout_marginLeft="50dip"/>
                    <ImageView
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dip"
                        android:background="@drawable/icon_disclosure"/>
                </RelativeLayout>

            </LinearLayout>






            <!--浏览足迹-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dip"
                android:background="@color/cell_background"
                android:layout_marginTop="10dip"
                android:id="@+id/liulanzuji">
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_centerVertical="true"
                    android:background="@drawable/icon_histroy"
                    android:layout_marginLeft="15dip"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="浏览足迹"
                    android:textColor="@color/text_color_0"
                    android:textSize="@dimen/txtsize_cell"
                    android:layout_marginLeft="50dip"/>
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dip"
                    android:background="@drawable/icon_disclosure"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="@color/line_color"
                />
            <!--关于我们-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dip"
                android:background="@color/cell_background"
                android:id="@+id/about_us">
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_centerVertical="true"
                    android:background="@drawable/icon_about"
                    android:layout_marginLeft="15dip"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="关于我们"
                    android:textColor="@color/text_color_0"
                    android:textSize="@dimen/txtsize_cell"
                    android:layout_marginLeft="50dip"/>
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dip"
                    android:background="@drawable/icon_disclosure"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="@color/line_color"
                />
            <!--推广邀请-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dip"
                android:background="@color/cell_background"
                android:id="@+id/tuiguangyaoqing">
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_centerVertical="true"
                    android:background="@drawable/icon_promot"
                    android:layout_marginLeft="15dip"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="推广邀请"
                    android:textColor="@color/text_color_0"
                    android:textSize="@dimen/txtsize_cell"
                    android:layout_marginLeft="50dip"/>
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dip"
                    android:background="@drawable/icon_disclosure"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="@color/line_color"
                />
            <!--使用帮助-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dip"
                android:background="@color/cell_background"
                android:id="@+id/shiyongbangzhu">
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_centerVertical="true"
                    android:background="@drawable/icon_guide"
                    android:layout_marginLeft="15dip"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="使用帮助"
                    android:textColor="@color/text_color_0"
                    android:textSize="@dimen/txtsize_cell"
                    android:layout_marginLeft="50dip"/>
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dip"
                    android:background="@drawable/icon_disclosure"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:background="@color/line_color"
                />
            <!--联系客服-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dip"
                android:background="@color/cell_background"
                android:id="@+id/contact_server">
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_centerVertical="true"
                    android:background="@drawable/icon_contact"
                    android:layout_marginLeft="15dip"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="联系客服"
                    android:textColor="@color/text_color_0"
                    android:textSize="@dimen/txtsize_cell"
                    android:layout_marginLeft="50dip"/>
                <ImageView
                    android:layout_width="20dip"
                    android:layout_height="20dip"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dip"
                    android:background="@drawable/icon_disclosure"/>
            </RelativeLayout>

            <!--退出当前账户-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dip"
                android:id="@+id/login_out"
                android:layout_marginTop="10dip"
                android:background="@color/cell_background">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="退出当前账户"
                    android:layout_centerInParent="true"
